{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node , express and api",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server.js",
    "start:prod": "SET NODE_ENV=production&& nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "import:data": "node dev-data/data/import-dev-data.js --import",
    "delete:data": "node dev-data/data/import-dev-data.js --delete",
    "format:pug": "../node_modules/.bin/prettier --write \"**/*.pug\" "
  },
  "keywords": [
    "nodejs",
    "api",
    "express"
  ],
  "author": "Fernando Cutire",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@prettier/plugin-pug": "^1.8.1",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.1",
    "hpp": "^0.2.3",
    "html-to-text": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mapbox": "^1.0.0-beta10",
    "mongoose": "^5.10.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "nodemailer": "^6.4.11",
    "pug": "^3.0.0",
    "sharp": "^0.26.2",
    "slugify": "^1.4.5",
    "stripe": "^8.125.0",
    "validator": "^13.1.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2"
  },
  "engines": {
    "node": "^14"
  }
}
